{
  "apiVersion": "1",
  "name": "b-blocks/fb-button",
  "title": "fb-button",
  "description": "Easy-to-use, responsive fb-button",
  "category": "widgets",
  "keywords": [
    "fb-button",
    "map",
    "Gutenberg"
  ],
  "textdomain": "fb-button",
  "attributes": {
    "align": {
      "type": "string",
      "default": ""
    },
    "cId": {
      "type": "string",
      "default": ""
    }, 
    "location": {
      "type": "string",
      "default": "Dhaka"
    },
    "zoom": {
      "type": "number",
      "default": 10
    },
    "alignment": {
      "type": "string",
      "default": "center"
    },
    "fbUrl":{
