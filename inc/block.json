{
  "apiVersion": "1",
  "name": "b-blocks/fb-button",
  "title": "fb-button",
  "description": "Easy-to-use, responsive fb-button",
  "category": "widgets",
  "keywords": [
    "fb-button",
    "map",
    "Gutenberg"
  ],
  "textdomain": "fb-button",
  "attributes": {
    "align": {
      "type": "string",
      "default": ""
    },
    "cId": {
      "type": "string",
      "default": ""
    },
    "location": {
      "type": "string",
      "default": "Dhaka"
    },
    "zoom": {
      "type": "number",
      "default": 10
    },
    "alignment": {
      "type": "string",
      "default": "center"
    },
    "fbUrl": {
      "type": "string",
      "default": "https://www.facebook.com/profile.php?id=100076228742365"
    },
    "btnType": {
      "type": "string",
      "default": "recommend"
    },
    "layout": {
      "type": "string",
      "default": "button"
    },
    "shareOffOn": {
      "type": "string",
      "default": "true"
    },
    "showFaces": {
      "type": "string",
      "default": "true"
    },
    "clrScheme": {
      "type": "string",
      "default": "dark"
    },
    "size": {
      "type": "string",
      "default": "large"
    },
    "background": {
      "type": "object",
      "default": {
        "color": "#0000"
      }
    },
    "padding": {
      "type": "object",
      "default": {
        "top": "5px",
        "right": "5px",
        "bottom": "5px",
        "left": "5px"
      }
    }
  },
  "supports": {
    "align": [
      "wide",
      "full"
    ],
    "html": false
  },
  "example": {
    "attributes": {
      "preview": true,
      "columns": {
        "desktop": 1,
        "tablet": 1,
        "mobile": 1
      }
    }
  },
  "editorScript": "file:../dist/editor.js"
}